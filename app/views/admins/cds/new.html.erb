<div class="container">
	<div class="card">
	  <%= render 'admins/artists/new' %>
	</div>
<p></p>
	<div class="card">
		<div class="card-body">
			<h1 class="card-title">CDページ</h1>
			<h2 class="card-subtitle mb-2 text-muted">CDを追加</h2>
			<%= form_for ([:admins, @cd]) do |f| %>
			 <p>【必須】シングル/アルバム名</p>
			 <%= f.text_field :single_album_name %>
			 <p>【必須】アーティスト</p>
			 <%= f.collection_select :artist_id, Artist.all, :id, :artist_name %>
			 <p>【必須】レーベル名</p>
			 <%= f.collection_select :label_id, Label.all, :id, :label %>
			 <p>【必須】ジャンル</p>
			 <%= f.collection_select :genre_id, Genre.all, :id, :genre %>
			 <p>【必須】価格</p>
			 <%= f.text_field :price %>
			 <p>【必須】発売日</p>
			 <%= f.text_field :release %>
			 <p>【必須】在庫</p>
			 <%= f.text_field :stock %>
			<%= f.submit '登録' %>
			<% end %>
		</div>
	</div>


</div>